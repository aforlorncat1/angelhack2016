<p id="notice"><%= notice %></p>

<h1>Listing Noms</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Votecount</th>
      <th>Restaurant</th>
      <th>Food</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @noms.each do |nom| %>
      <tr>
        <td><%= nom.price %></td>
        <td><%= nom.vote_count %></td>
        <td><%= nom.restaurant_id %></td>
        <td><%= nom.food_id %></td>
        <td><%= link_to 'Show', nom %></td>
        <td><%= link_to 'Edit', edit_nom_path(nom) %></td>
        <td><%= link_to 'Destroy', nom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<%= link_to 'New Nom', new_nom_path %>
